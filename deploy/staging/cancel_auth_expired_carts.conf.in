[loggers]
keys = root, ticketing

[handlers]
keys = rotate

[formatters]
keys = generic

[logger_root]
level = INFO
handlers = rotate

[logger_ticketing]
level = DEBUG
handlers = rotate
qualname = ticketing
propagate = 0

[handler_rotate]
class = logging.handlers.RotatingFileHandler
args = ("${buildout:directory}/var/log/cancel_auth_expired_carts",)
level = NOTSET
formatter = generic


[formatter_generic]
format = %(asctime)s %(levelname)-5.5s [%(name)s][%(threadName)s] %(message)s

# End logging configuration
