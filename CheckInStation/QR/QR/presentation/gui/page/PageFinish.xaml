<Page x:Class="QR.presentation.gui.page.PageFinish"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:control="clr-namespace:QR.presentation.gui.control"
      mc:Ignorable="d" 
      d:DesignHeight="50" d:DesignWidth="50"
    ShowsNavigationUI="False"     
	Title="PageFinish"
    Loaded="OnLoaded">
    <Grid Style="{StaticResource ToplevelGrid}">
        <Grid Style="{StaticResource CenterContentGrid}">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
       <control:InspectCase Grid.Row="0"></control:InspectCase>
            <TextBlock Grid.Row="1">
                <TextBlock.Text>
                    <MultiBinding StringFormat="status:{0}}">
                        <Binding Path="Status" Mode="OneWay"></Binding>
                    </MultiBinding>                    
                </TextBlock.Text>
            </TextBlock>
        </Grid>
    </Grid>
</Page>
