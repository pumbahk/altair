<Page x:Class="QR.presentation.gui.page.PageDummy"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:c="clr-namespace:QR.presentation.gui.control"
      mc:Ignorable="d" 
      d:DesignHeight="50" d:DesignWidth="50"
    ShowsNavigationUI="False"
    FocusManager.FocusedElement="{Binding ElementName=Input}"
	Title="PageOrdernoTelInput">

    <Grid>
        <StackPanel>
           <c:InspectCase Grid.Row="0"></c:InspectCase> 
            <c:KeyPad KeyPadFinish="KeyPad_KeyPadFinish">
                <c:TelPad></c:TelPad>
            </c:KeyPad>
            <Button>進む</Button>
            <Button>戻る</Button>
        </StackPanel>
    </Grid>
</Page>
